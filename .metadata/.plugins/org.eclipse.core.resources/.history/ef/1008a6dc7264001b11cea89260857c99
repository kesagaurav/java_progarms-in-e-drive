
public class Main {

	private String name;
	private int accno;
	
	public void setbank(String name,int accno) {
		name=name;
		accno=accno;
	}
	
	
	public void showbank() {
		System.out.println("\n name="+name +"\n accno"+ name);
	}
	
	
	
	public static void main(String[] args) {
		
		Main obj=new Main();
		obj.setbank("gaurav", 101);
		System.out.println("\n bank details are");
		obj.showbank();

	}

}
